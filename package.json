{
  "name": "le-french-language",
  "displayName": "Le French Language",
  "description": "Language support for french pseudo-code",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mojmilo/vscode-algorithme-color/"
  },
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "txt",
        "aliases": [
          "txt",
          "txt"
        ],
        "extensions": [
          "txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "plaintext",
        "scopeName": "txt",
        "path": "./syntaxes/le-french.tmLanguage.json"
      }
    ]
  }
}
