{
  "name": "le-french-language",
  "displayName": "Le French Language",
  "description": "Language support for french pseudo-code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "txt",
      "aliases": ["txt", "txt"],
      "extensions": ["txt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "plaintext",
      "scopeName": "txt",
      "path": "./syntaxes/le-french.tmLanguage.json"
    }]
  }
}
